          <ToolTip />
          <Default>cur</Default>
        </Literal>
      </Declarations>
      <Code Language="sql"><![CDATA[DECLARE $cur$ CURSOR LOCAL FAST_FORWARD FOR
SELECT 1
FROM @table

OPEN $cur$

WHILE 1=1
BEGIN
	FETCH NEXT FROM $cur$ INTO @var
	
	IF @@FETCH_STATUS <> 0 BREAK
END

CLOSE $cur$
DEALLOCATE $cur$]]></Code>
    </Snippet>
